{
  "expo": {
    "name": "Aquazoo",
    "scheme": "appv1",
    "slug": "app_v1",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/appicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/aquazoologo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.aquazoo.zooapp",
      "buildNumber": "31",
      "config": {
        "googleMapsApiKey": "AIzaSyBN2fxQjmuQ7fdTE4szAZVrRwUtTIQ_ME4"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "AquaZoo uses your camera to allow you to take photos for the interactive animal photo snap games.",
        "NSLocationWhenInUseUsageDescription": "AquaZoo uses your live location to display nearby zoo areas on the map and enhance your way finding experience around the zoo.",
        "NSPhotoLibraryUsageDescription": "AquaZoo needs access to your photo library so you can save and view the animal photos you capture in the app.",
        "UIRequiresFullScreen": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.immraj.app_v1",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDsREq71IL0oAyTucaHmf46PSlTQumCA5Y"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-document-picker"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1e2466c8-01fd-4bcd-a066-354d6d9a99ab"
      }
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ]
  }
}
