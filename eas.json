{
  "build": {
    "preview": {
      "android": {
        "image": "latest",
        "buildType": "apk"
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    },
    "gradle-release": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    },
    "development": {
      "android": {
        "developmentClient": true
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    },
    "internal": {
      "android": {
        "distribution": "internal"
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    },
    "production": {
      "android": {
        "image": "ubuntu-22.04-android-ndk-r25b",
        "buildType": "app-bundle"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    }
  }
}
