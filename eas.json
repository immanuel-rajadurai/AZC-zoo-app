{
  "build": {
    "preview": {
      "android": {
        "image": "latest",
        "buildType": "apk"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    },
    "gradle-release": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    },
    "development": {
      "android": {
        "developmentClient": true
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    },
    "internal": {
      "android": {
        "distribution": "internal"
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    },
    "production": {
      "android": {
        "image": "latest",
        "buildType": "app-bundle"
      },
      "ios": {
        "image": "latest",
        "distribution": "store"
      },
      "env": {
        "EAS_NO_DEFAULT_NPM_INSTALL": "1"
      }
    }
  },
  "submit": {
    "preview": {
      "android": {},
      "ios": {}
    },
    "gradle-release": {
      "android": {},
      "ios": {}
    },
    "development": {
      "android": {},
      "ios": {}
    },
    "internal": {
      "android": {
        "track": "internal"
      },
      "ios": {}
    },
    "production": {
      "android": {
        "track": "production"
      },
      "ios": {
        "appleId": "immanuelraj154@gmail.com",
        "ascAppId": "6739294395"
      }
    }
  }
}
